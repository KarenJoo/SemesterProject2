  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarenJoo/SemesterProject2.git"
  },
  "keywords": [
    "app",
    "bootstrap",
    "css",
    "html",
    "js"
  ],
  "author": "Karen Jo <karenfiksdahl@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KarenJoo/SemesterProject2/issues"
  },
  "homepage": "https://github.com/KarenJoo/SemesterProject2#readme",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "popper.js": "^1.16.1",
    "sass": "^1.69.5"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "3.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
