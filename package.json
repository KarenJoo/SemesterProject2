{
  "name": "semester_project_2",
  "version": "0.0.6",
  "description": "Semester Project 2 BestBid auction app ",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "sass src/scss:dist/css",
    "watch": "sass --watch src/scss:dist/css",
